{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "sql:allow-select",
    "sql:allow-execute",
    "sql:allow-load",
    "dialog:allow-save",
    "dialog:allow-confirm",
    "dialog:allow-ask",
    "dialog:allow-message",
    "dialog:allow-open",
    {
      "identifier": "fs:allow-write-text-file",
      "scope": [
        "**"
      ]
    },
    {
      "identifier": "fs:allow-read-text-file",
      "scope": [
        "**"
      ]
    },
    {
      "identifier": "fs:allow-read-file",
      "scope": [
        "**",
        "$APPLOCALDATA/**",
        "$APPLOCALDATA/pensionsmanager.db",
        "C:/Users/fluxt/AppData/Local/com.pensionsmanager.app/**"
      ]
    },
    {
      "identifier": "fs:allow-write-file",
      "scope": [
        "**",
        "$APPLOCALDATA/**",
        "$APPLOCALDATA/pensionsmanager.db",
        "C:/Users/fluxt/AppData/Local/com.pensionsmanager.app/**"
      ]
    },
    {
      "identifier": "fs:allow-copy-file",
      "scope": [
        "**",
        "$APPLOCALDATA/**",
        "$APPLOCALDATA/pensionsmanager.db",
        "C:/Users/fluxt/AppData/Local/com.pensionsmanager.app/**"
      ]
    },
    {
      "identifier": "fs:allow-exists",
      "scope": [
        "**",
        "$APPLOCALDATA/**",
        "$APPLOCALDATA/pensionsmanager.db",
        "C:/Users/fluxt/AppData/Local/com.pensionsmanager.app/**"
      ]
    }
  ]
}